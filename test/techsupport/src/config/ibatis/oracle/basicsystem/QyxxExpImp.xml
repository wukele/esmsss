	<!-- 公共 查询条件 -->
	<sql id="t_allPageWhere" >
		<isNotEmpty prepend="and" property="cxtjDcbs">
			a.dcbs = #cxtjDcbs#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="cxtjDcsj">
			a.dcsj &gt;=#cxtjDcsj:DATE# and a.dcsj &lt; #cxtjDcsj:DATE#+1
		</isNotEmpty>
	</sql>
	<!-- 公共模块 查询条件 -->
	<sql id="t_publicPageWhere" >
		<isNotEmpty prepend="and" property="qybm">
			a.qybm like '%$qybm$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="qymc">
			a.qymc like '%$qymc$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="gxdwbm">
			a.gxdwbm like '$gxdwbm$%'
		</isNotEmpty>
	</sql>
	<!-- 旅馆业 查询条件 -->
	<sql id="t_lgyPageWhere" >
		<isNotEmpty prepend="and" property="qybm">
			a.qiyebianma like '%$qybm$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="qymc">
			a.qiyemc like '%$qymc$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="gxdwbm">
			a.gxdwbm like '$gxdwbm$%'
		</isNotEmpty>
	</sql>
	<!-- 典当业 查询条件 -->
	<sql id="t_ddyPageWhere" >
		<isNotEmpty prepend="and" property="qybm">
