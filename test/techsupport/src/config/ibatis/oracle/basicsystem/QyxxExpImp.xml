			<include refid="t_czfwyPageWhere"/>
		</dynamic>
	</select>
	
	
	<!-- 旅馆业 导出标识修改 -->
	<update id="updateLgyxx" parameterClass="qyxxExpImp">
		update t_lvguan a set a.zhuxiaobz = 0 
		<dynamic prepend="">
			<isNotEmpty prepend="," property="qyid">
				a.qiyeid = #qyid#
			</isNotEmpty>
			<isNotEmpty prepend="," property="qybm">
				a.qiyebianma = #qybm#
			</isNotEmpty>
			<isNotEmpty prepend="," property="dcbs">
				a.dcbs=#dcbs#
			</isNotEmpty>
			<isNotEmpty prepend="," property="dcsj">
				a.dcsj = #dcsj:DATE#
			</isNotEmpty>
			<isNotEmpty prepend="," property="wwtbbz">
				a.waiwangtbbz=#wwtbbz#
			</isNotEmpty>
			<isNotEmpty prepend="," property="encodestr">
				a.jiqijiamibh=#encodestr#
			</isNotEmpty>
		</dynamic>
		where 1=1 and a.zhuxiaobz = 0 
		<dynamic prepend="">
			<include refid="t_allPageWhere"/>
			<include refid="t_lgyPageWhere"/>
			<isNotEmpty prepend="and" property="qyid"><!-- 单条修改 -->
				a.qiyeid = #qyid# 
			</isNotEmpty>
			<isNotEmpty prepend="and" property="qybm"><!-- 单条修改 -->
				a.qiyebianma = #qybm# 
			</isNotEmpty>
		</dynamic>
	</update>
	<!-- 旅馆业 xml数据查询 不分页 -->
	<select id="getLgyXmlMes" resultMap="lgyXmlMesResult" parameterClass="qyxxExpImp">
		select a.qiyeid qyid,a.hangyelx qylx,a.qiyebianma qybm,a.qiyemc qymc,
		a.qitaisblx qycplxbm,a.farenmc frxm,a.zongjingli zjl,
		a.zhianzrr zafzr,a.xiangxidizhi dizhi,a.lianxidh lxdh,
		a.baoanbudh babdh,a.zhuangtai qyzt,a.zhuangtaigbrq qyztgbrq,
		a.jiqijiamibh jqbm,a.gxdwbm gxdwbm,a.gxdwmc gxdwmc,
		a.qiyexingzhi_dm qyxz,a.congyerenshu cyrs,a.zhibaorenshu zbrys,
		a.jingpeixunrs jpxrs,a.dengjiriqi fwksrq,a.fangjianshu fjs,
		a.chuangweishu cws,a.qiyexingji bgxj,a.qiyedengji bgdj,a.yewulbbm zflbm 
		from t_lvguan a 
		where a.zhuxiaobz = 0 
		<dynamic prepend="">
			<include refid="t_allPageWhere"/>
			<include refid="t_lgyPageWhere"/>
			<isNotEmpty prepend="and" property="qyid"><!-- 单条查询 -->
				a.qiyeid = #qyid# 
			</isNotEmpty>
		</dynamic>
		order by a.qiyeid desc
	</select>
	<!-- 旅馆业 xml数据查询 分页 -->
	<select id="getLgyXmlMesPage" resultMap="lgyXmlMesResult" parameterClass="map">
		select x.* from (
		select rownum rowno,z.* from (
		select a.qiyeid qyid,a.hangyelx qylx,a.qiyebianma qybm,a.qiyemc qymc,
		a.qitaisblx qycplxbm,a.farenmc frxm,a.zongjingli zjl,
		a.zhianzrr zafzr,a.xiangxidizhi dizhi,a.lianxidh lxdh,
		a.baoanbudh babdh,a.zhuangtai qyzt,a.zhuangtaigbrq qyztgbrq,
		a.jiqijiamibh jqbm,a.gxdwbm gxdwbm,a.gxdwmc gxdwmc,
		a.qiyexingzhi_dm qyxz,a.congyerenshu cyrs,a.zhibaorenshu zbrys,
		a.jingpeixunrs jpxrs,a.dengjiriqi fwksrq,a.fangjianshu fjs,
		a.chuangweishu cws,a.qiyexingji bgxj,a.qiyedengji bgdj,a.yewulbbm zflbm
		from t_lvguan a 
		where a.zhuxiaobz = 0 
		<dynamic prepend="">
			<include refid="t_allPageWhere"/>
			<include refid="t_lgyPageWhere"/>
		</dynamic>
		order by a.qiyeid desc
		)  z) x where rowno &gt;$beginRow$ and rowno &lt;= $endRow$ 
	</select>
