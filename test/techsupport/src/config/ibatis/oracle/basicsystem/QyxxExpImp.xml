		select rownum rowno,z.* from (
		select a.WYBZ qyid,a.CAIJIDIANBM qybm,a.CAIJIDIANMC qymc,
		a.FUZEREN frxm,a.LIANXIDH lxdh,a.XIANGXIDIZHI dizhi,a.GXDWMC gxdwmc 
		from t_caijidian a where a.ISZHUXIAO = 0
		<dynamic prepend="">
			<include refid="t_allPageWhere"/>
