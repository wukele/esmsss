	</sql>
	<!-- 出租房屋业 查询条件 -->
	<sql id="t_czfwyPageWhere" >
		<isNotEmpty prepend="and" property="qybm">
			a.CAIJIDIANBM like '%$qybm$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="qymc">
			a.CAIJIDIANMC like '%$qymc$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="gxdwbm">
			a.GXDWBM  like '$gxdwbm$%'
		</isNotEmpty>
	</sql>
	<!-- 公用模块 企业信息 -->
	<select id="getPublicQyxxList" resultMap="qyxxListResult" parameterClass="map">
		select x.* from (
		select rownum rowno,z.* from (
		select a.qyid qyid,a.qybm qybm,a.qymc qymc,
		a.frdb frxm,a.lxdh lxdh,a.jydz dizhi,a.gxdwmc gxdwmc
		from t_qyjbxx a
		where a.zxbz = 0 and a.scbz = 0 and a.hylbdm = #hylbdm#
		<dynamic prepend="">
			<include refid="t_allPageWhere"/>
			<include refid="t_publicPageWhere"/>
		</dynamic>
		order by a.qyid desc )  z) x where rowno &gt;$beginRow$ and rowno &lt;= $endRow$ 
	</select>
	<!-- 公用模块 企业信息 -->
	<select id="getPublicQyxxListCount" resultClass="int" parameterClass="map">
		select count(1) a from t_qyjbxx a
		where a.zxbz = 0 and a.scbz = 0 and a.hylbdm = #hylbdm#
		<dynamic prepend="">
			<include refid="t_allPageWhere"/>
			<include refid="t_publicPageWhere"/>
		</dynamic>
	</select>
	
