			<include refid="t_lgyPageWhere"/>
		</dynamic>
	</select>
	
	<!-- 公共模块 导出标识修改 -->
	<update id="updatePublicQyxx" parameterClass="qyxxExpImp">
		update t_qyjbxx a set a.scbz = 0
			<isNotEmpty prepend="," property="qyid">
				a.qyid = #qyid# 
			</isNotEmpty>
			<isNotEmpty prepend="," property="qybm">
				a.qybm = #qybm# 
			</isNotEmpty>
			<isNotEmpty prepend="," property="dcbs">
				a.dcbs = #dcbs#
			</isNotEmpty>
			<isNotEmpty prepend="," property="dcsj">
				a.dcsj = #dcsj:DATE#
			</isNotEmpty>
		where a.zxbz = 0 and a.scbz = 0 and a.hylbdm = #hylbdm#
		<dynamic prepend="">
			<include refid="t_allPageWhere"/>
			<include refid="t_publicPageWhere"/>
			<isNotEmpty prepend="and" property="qyid"><!-- 单条修改 -->
				a.qyid = #qyid# 
			</isNotEmpty>
			<isNotEmpty prepend="and" property="qybm"><!-- 单条修改 -->
				a.qybm = #qybm# 
			</isNotEmpty>
		</dynamic>
	</update>
	<!-- 公共模块 xml数据查询 -->
