	<select id="getWscCsqkList" resultMap="wsccsqkListResult" parameterClass="map">
		select x.* from (
		select rownum rowno,z.* from 
		(
  SELECT
       Q.QYID    QYID, 
       Q.QYBM    QYBM,
       Q.QYMC    QYMC,
       Q.QYZFLMC QYZFLMC,
       Q.YYZTMC  YYZTMC,
       Q.ZRS     ZRS,
       W.YLJTS   YLJTS,
       W.NLJTS   NLJTS
  FROM T_QYJBXX Q,
       (SELECT A.QYBM QYBM,
               COUNT(CASE
                       WHEN A.WSCRQ &gt;= TRUNC(SYSDATE, 'mm') AND A.WSCRQ &lt;= SYSDATE THEN
                        A.QYBM
                       ELSE
                        NULL
                     END) YLJTS,
               COUNT(CASE
                       WHEN A.WSCRQ &gt;= TRUNC(SYSDATE, 'yyyy') AND
                            A.WSCRQ &lt;= SYSDATE THEN
                        A.QYBM
                       ELSE
                        NULL
                     END) NLJTS
        
          FROM T_WSCQY A
         GROUP BY A.QYBM) W
 WHERE W.QYBM = Q.QYBM
   AND Q.HYLBDM = 'J'
   AND (Q.ZT = '1' OR Q.ZT = '2')
   AND Q.SCBZ = 0
			<isNotEmpty prepend="and" property="deptCode">
				Q.LRDWBM LIKE '$deptCode$%'
			</isNotEmpty>
  ORDER BY W.QYBM
		)  z) x where rowno &gt;$beginRow$ and rowno &lt;= $endRow$ 
	</select>
