			</isNotEmpty>
			<isNotEmpty prepend="and" property="hylbdm">
 				a.hylbdm =#hylbdm#
 			</isNotEmpty>
			<include refid="t_wgcyryxxGgPageWhere" />
	</sql>
	
	
