		<sql id="t_wgcyryxxGgPageTwoWhere">
			<isNotEmpty prepend="and" property="qybm">
				b.qybm like '%$qybm$%'
			</isNotEmpty>
			<isNotEmpty prepend="and" property="qymc">
				b.qymc like '%$qsmc$%'
			</isNotEmpty>
			<isNotEmpty prepend="and" property="ywx">
