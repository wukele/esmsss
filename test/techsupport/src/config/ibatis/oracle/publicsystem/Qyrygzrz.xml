	<select id="getQyrygzrzList" resultMap="qyrygzrzPageResult" parameterClass="map">
		select x.* from (
		select rownum rowno,z.* from 
		(select a.qyid qyid,a.ryid ryid,a.gzrq gzrq,a.qyrygzrzid qyrygzrzid,
