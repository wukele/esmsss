		</isNotEmpty>
		
	   from t_qyryxxtj a ,t_qyjbxx b
		where 1=1 and a.qybm=b.qybm
	   <dynamic prepend="">
			<include refid="t_qyryxxtjWhereTwo" />
		</dynamic>
		<isNotEmpty prepend=" " property="groupBy">
