	<select id="getClxx_zpList" resultMap="clxx_zpListResult" parameterClass="map">
		select x.* from (
		select rownum rowno,z.* from 
		(select a.cljbxxid cljbxxid,a.clxxzpid clxxzpid,a.tpmc tpmc,a.tplx tplx,a.tpnr tpnr
