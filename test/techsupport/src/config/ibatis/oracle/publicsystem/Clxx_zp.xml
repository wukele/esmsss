	<select id="getClxx_zp" resultMap="clxx_zpResult" parameterClass="clxx_zp">
		select a.cljbxxid cljbxxid,a.clxxzpid clxxzpid,a.tpmc tpmc,a.tplx tplx,a.tpnr tpnr
