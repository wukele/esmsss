			</isNotEmpty>
			
			<isNotEmpty prepend="and" property="qsrq">
			<![CDATA[
 				a.tjrq >= #qsrq:DATE#
 			]]>
		</isNotEmpty>
		<isNotEmpty prepend="and" property="jzrq">
