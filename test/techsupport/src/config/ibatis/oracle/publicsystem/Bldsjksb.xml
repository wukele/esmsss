	<!--闭路电视监控设备(t_bldsjksb)公安端闭路电视监控设备查询-->
	<select id="getBldsjksbGadList" resultMap="bldsjksbGadListResult" parameterClass="map">
		select x.* from (
		select rownum rowno,z.* from 
		(select a.qyid qyid,a.bldsjksbid bldsjksbid,a.wlcslxbm wlcslxbm,a.wlcslxmc wlcslxmc,
		a.sbjcjgmc sbjcjgmc,a.azsxtsl azsxtsl,a.sbxh sbxh,a.sccsmc sccsmc,a.ggxh ggxh,
		a.azsl azsl,a.sbmc sbmc,a.sflx sflx,a.ipdz ipdz,a.anwz anwz,a.zt zt,a.tyyy tyyy,a.tysj tysj,a.jibr jibr,a.bz bz,a.lrr lrr ,a.lrsj lrsj,
		b.qybm,b.qymc,b.qyzflbm,b.qyzflmc
		from t_bldsjksb a ,t_qyjbxx b
		where 1=1 and a.qyid=b.qyid
		<dynamic prepend="">
			<isNotEmpty prepend="and" property="superbWhere">
				$superbWhere$
			</isNotEmpty>
			<isNotEmpty prepend="and" property="ggxh">
				a.ggxh like '%$ggxh$%'
			</isNotEmpty>
			<isNotEmpty prepend="and" property="sbmc">
				a.sbmc like '%$sbmc$%'
			</isNotEmpty>
			<isNotEmpty prepend="and" property="qybm">
				b.qybm like '%$qybm$%'
			</isNotEmpty>
			<isNotEmpty prepend="and" property="qymc">
				b.qymc=#qymc#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="sfyx"><!-- 是否有效（公安端是否可见） 0-无效，1-有效 -->
				<isEqual prepend="" property="sfyx"  compareValue="0">
					 b.zt = '0'
				</isEqual>
				<isEqual prepend="" property="sfyx"  compareValue="1">
					  b.zt &lt;&gt; '0' and b.zt &lt;&gt; '3'
				</isEqual>
     		</isNotEmpty>
     		<isNotEmpty prepend="and" property="hylbdm">
				b.hylbdm=#hylbdm#
			</isNotEmpty>
     		<isNotEmpty prepend="and" property="allhylbdm">
				b.hylbdm in ($allhylbdm$)
			</isNotEmpty>
			<isNotEmpty prepend="and" property="deptCode">
				b.gxdwbm like '$deptCode$%'
			</isNotEmpty>
			<include refid="t_bldsjksbPageWhere" />
			<isNotEmpty prepend="and" property="zt">
				a.zt=#zt#
			</isNotEmpty>
		</dynamic>
		order by $pageSort$)  z) x where rowno &gt;$beginRow$ and rowno &lt;= $endRow$ 
	</select>
