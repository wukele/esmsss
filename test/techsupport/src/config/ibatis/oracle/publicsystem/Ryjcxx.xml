	<select id="getRyjcxxList" resultMap="ryjcxxListResults" parameterClass="map">
		select x.* from (
		select rownum rowno,z.* from 
		(select a.qyid qyid,a.ryid ryid,a.rycfid rycfid,a.hylbdm hylbdm,a.hylb hylb,a.ryjcrq ryjcrq,a.cfdjxh cfdjxh,a.cflbbm cflbbm,
		a.cflb cflb,a.jcmxlbdm jcmxlbdm,a.jcmxlb jcmxlb,a.xzcfpzwh xzcfpzwh,a.pzjgdm pzjgdm,a.pzjg pzjg,a.pzrbh pzrbh,a.pzrxm pzrxm,
		a.pzrq pzrq,a.zxrbh zxrbh,a.zxrxm zxrxm,a.zxrq zxrq,a.qkms qkms,a.yjly yjly,a.jcfd jcfd,a.scbz,b.qybm,b.qymc,b.gxdwmc,c.xm,c.zjhm,c.xb,c.cylb,c.cyrybh
		from t_ryjcxx a, t_qyjbxx b, t_qyryxx c
		where a.qyid=b.qyid and a.ryid=c.ryid and a.scbz=0
		<dynamic prepend="">
		    <isNotEmpty prepend="and" property="hylbdm">
				a.hylbdm = #hylbdm#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="cyrybh">
				c.cyrybh =#cyrybh#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="cylbdm">
				c.cylbdm = #cylbdm#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="qybm">
				b.qybm = #qybm#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="qymc">
				b.qymc like '%$qymc$%'
			</isNotEmpty>
		    <isNotEmpty prepend="and" property="xm">
				c.xm = #xm#
			</isNotEmpty>
		    <isNotEmpty prepend="and" property="cflbbm">
				a.cflbbm = #cflbbm#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="jcmxlbdm">
				a.jcmxlbdm = #jcmxlbdm#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="ryjcrqf">
				a.ryjcrq &gt;=#ryjcrqf:DATE#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="ryjcrqt">
				a.ryjcrq &lt; #ryjcrqt:DATE#+1
			</isNotEmpty>
			<isNotEmpty prepend="and" property="gxdwbm">
				b.gxdwbm like '%$gxdwbm$%'
			</isNotEmpty>
			<isNotEmpty prepend="and" property="zjhm">
				c.zjhm=#zjhm#
			</isNotEmpty>
			<include refid="t_ryjcxxPageWhere" />
		</dynamic>
		order by $pageSort$)  z) x where rowno &gt;$beginRow$ and rowno &lt;= $endRow$ 
	</select>
