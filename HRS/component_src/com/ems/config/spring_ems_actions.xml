<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
		
	
	

	
	
	<bean id="LoadMenuItems" class="com.ulp.action.LoadMenuItemsAction"
		abstract="false"  lazy-init="default"
		autowire="default" dependency-check="default">
		<property name="menuDao">
			<ref bean="MenuItemDao" />
		</property>
	</bean>
	
	
	
	
	<bean id="EmsDispatcherPageAction"
		class="com.ems.component.action.EmsDispatcherPageAction"
		abstract="false" lazy-init="default" autowire="default"
		dependency-check="default">
		<property name="pageManager">
			<ref bean="PageResourcesManager" />
		</property>
		<property name="engineRes">
			<ref bean="PageEngineManager" />
		</property>
	</bean></beans>