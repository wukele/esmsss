	<package name="UserAdmin" extends="struts-default">
		<action name="user_manager_main_view" method="displayUserMain"
			class="UserManageAction">
			<result>/pages/admin/userAdmin/UserMain/UserMain.jsp</result>
		</action>
	</package>
</struts>    
