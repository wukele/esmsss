<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<constant name="struts.devMode" value="false" />
	<constant name="struts.objectFactory" value="spring" />
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.i18n.encoding" value="GBK" />

	<package name="base" namespace="/" extends="struts-default">
		<action name="login" class="LoginAction">
			<result name="success" type="chain">
				<param name="namespace">/</param>
				<param name="actionName">LoadMenuItems</param>
			</result>
			<result name="error">/pages/Login/errorLogin.jsp</result>
		</action>
		<action name="LoadMenuItems" class="LoadMenuItems">
			<result name="success">/index.jsp</result>
		</action>
		<action name="home" class="com.opensymphony.xwork2.ActionSupport">
			<result>/pages/Home.jsp</result>
		</action>
	</package>
	<!--<package name="component" namespace="/" extends="struts-default">
		<action name="ems_dispatcher_page_action" class="EmsDispatcherPageAction">
			<result>/pages/ems/comm/EmsDispatcherPage.jsp</result>
		</action>
	</package>-->
	<include file="com/environment/action/struts_environment.xml"></include>
	<include file="com/ulp/action/json/struts_ulpJson.xml"></include>
	 <include file="com/ems/component/action/struts_component_action.xml"></include> 
	  <include file="com/test/action/TestAction.struts.xml"></include>
</struts>    
